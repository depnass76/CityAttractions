@model IEnumerable<CityAttractions.Data.Thumbnail>




<h4 style="color:blue">@Model.Count() Favorite Cities</h4>


<div class="row">

    @foreach (var city in Model)
    {
        <div class="col-md-6">
            <div class="card card-body bg-light">
                <a href="@city.url">
                    <img src="@city.url" />
                </a>
            </div>
        </div>
    }
</div>














